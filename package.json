{
  "name": "@rookino/react-lottie-light",
  "version": "2.0.2",
  "private": false,
  "author": "perrmadiafrrian",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "components",
    "lottie"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/perrmadiafrrian/react-lottie-light.git"
  },
  "dependencies": {
    "lottie-web": "^5.7.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && cross-env NODE_ENV=production babel ./src --ignore tests,stories --out-dir dist && tsc",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o docs-build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.15.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "cross-env": "^7.0.3",
    "jest-canvas-mock": "^2.3.1",
    "typescript": "^4.3.5"
  }
}
